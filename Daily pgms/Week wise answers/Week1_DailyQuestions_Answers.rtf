\b0\par
public class IrctcSelectdropdown  \{\par
\par
\tab static int i=0;\par
\par
\tab public static void main(String[] args) \{\par
\par
\tab\tab // Launch browser\par
\tab\tab System.setProperty("webdriver.chrome.driver", "./drivers/chromedriver.exe");\par
\tab\tab ChromeDriver driver=new ChromeDriver();\par
\par
\tab\tab // load url\par
\tab\tab driver.get("https://www.irctc.co.in/eticketing/userSignUp.jsf");\par
\par
\tab\tab // implicitwait\par
\tab\tab driver.manage().timeouts().implicitlyWait(30, TimeUnit.SECONDS);\par
\par
\tab\tab // Identify the element and store\par
\tab\tab WebElement element=driver.findElementById("userRegistrationForm:nationalityId");\par
\par
\tab\tab // create an object using Select class for an webelement \par
\tab\tab Select dropdown=new Select(element);\par
\par
\tab\tab // get collection of webelement and store it in list\par
\tab\tab List<WebElement> options=dropdown.getOptions();\par
\par
\tab\tab //iterations through each webElement based on condection\par
\tab\tab for (WebElement eachoption : options) \{\par
\tab\tab\tab if (eachoption.getText().startsWith("E")) \{\par
\tab\tab\tab\tab i++;\par
\tab\tab\tab\tab if (i==2) \{\par
\tab\tab\tab\tab\tab System.out.println(eachoption.getText());\par
\tab\tab\tab\tab\tab eachoption.click();\par
\tab\tab\tab\tab\tab break;\par
\tab\tab\tab\tab\}\par
\tab\tab\tab\}\par
\tab\tab\}\par
\par
\tab\tab //close browser\par
